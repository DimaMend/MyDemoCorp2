{
  "settingsInheritedFrom": "DimaMend/whitesource-config@main",
  "scanSettings": {
    "enableReachability": true,
    "exploitability": true
  },
  "checkRunSettings": {
    "strictMode": "warning"
  },
  "scanSettingsSAST": {
    "enableScan": "true",
    "scanPullRequests": true
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": false
    },
    "enableRenovate": true,
    "extends": [
      "config:recommended",
      "mergeConfidence:all-badges",
      "packageRules": [
        {
      	  "groupName": "Mend: high confidence minor and patch dependency updates",
      	  "matchUpdateTypes": ["minor", "patch"],
      	  "matchConfidence": ["very high", "high"]
        },
        {
      	  "matchConfidence": ["low"],
      	  "dependencyDashboardApproval": true,
      	  "commitMessagePrefix": "[LOW] "
        },
        {
      	  "matchConfidence": ["neutral"],
      	  "dependencyDashboardApproval": true,
      	  "commitMessagePrefix": "[NEUTRAL] "
        }
      ]
    ]
  }
}
